{
  "name": "kni",
  "description": [
    "kni <script.kni>"
  ],
  "usage": [
    "<scripts>{1..} :input Script file name(s)",
    "[-s <start>] Start script at this label",
    "[-J] Read kni script as compiled JSON",
    "[-j|--json] Write compiled script as JSON.",
    "[-h|--html <script>] :output Write interactive HTML file.",
    "[-d] Describe instead of running interactively.",
    "[-t <script>] :output Write a transcript.",
    "[-v <script>] :input Verify a prior recorded transcript.",
    "[-S <seed>] :number Seed the random number generator.",
    "[-r <waypoint>] :input Restore a saved waypoint in the story.",
    "[--di] Log input file before processing",
    "[--dr] Enable runtime debug logging",
    "[--dp] Enable parser debug logging",
    "[--dil] Enable inline lexer debug logging",
    "[--dol] Enable outline lexer debug logging",
    "[--ds] Enable scanner debug logging",
    "[--html-title <title>] Set the title for the generated page.",
    "[--html-color <color>] Set the foreground color for the generated page.",
    "[--html-background-color <color>] Set the background color for the generated page."
  ],
